{
  "manifest_version": 3,
  "name": "网页监控助手",
  "version": "1.1",
  "description": "监控网页元素变化",
  "icons": {
  },
  "permissions": [
    "storage",
    "alarms",
    "activeTab",
    "tabs",
    "scripting",
    "downloads",
    "notifications"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    }
  },
  "background": {
    "service_worker": "background.js"
  }
} 